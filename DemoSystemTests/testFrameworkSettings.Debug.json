{
  "webSettings": {
    "executionType": "regular",
    "defaultBrowser": "chrome",
    "defaultLifeCycle": "restart everytime time",

    "shouldHighlightElements": "true",
    "shouldCaptureHttpTraffic": "false",

    "screenshotsOnFailEnabled": "true",
    "screenshotsSaveLocation": "${user.home}/POLARIS/Screenshots",
    "videosOnFailEnabled": "false",
    "videosSaveLocation": "${user.home}/POLARIS/Videos",
    "timeoutSettings": {
      "elementWaitTimeout": "30",
      "pageLoadTimeout": "30",
      "scriptTimeout": "1",
      "sleepInterval": "1",
      "validationsTimeout": "30",
      "elementToBeVisibleTimeout": "30",
      "elementToExistTimeout": "30",
      "elementToNotExistTimeout": "30",
      "elementToBeClickableTimeout": "30",
      "elementNotToBeVisibleTimeout": "30",
      "elementToHaveContentTimeout": "15"
    },
    "gridSettings": [
      {
        "providerName": "lambdatest",
        "optionsName": "LT:Options",
        "url": "https://{env_lt_username}:{env_lt_access_key}@hub.lambdatest.com/wd/hub",
        "arguments": [
          {
            "resolution": "1920x1080",
            "platform": "Windows 10",
            "browser": "chrome",
            "browserVersion": "114.0",
            "visual": "true",
            "video": "true",
            "build": "1.0",
            "project": "POLARIS_RUN",
            "username": "{env_lt_username}",
            "accessKey": "{env_lt_access_key}"
          }
        ]
      },
      {
        "providerName": "grid",
        "url": "http://127.0.0.1:4444/wd/hub",
        "arguments": [
          {
            "name": "bellatrix_run"
          }
        ]
      }
    ]
  }
}